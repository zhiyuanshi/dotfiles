[user]
	name = Zhiyuan Shi
	email = zhiyuanshi@users.noreply.github.com
[credential]
	helper = cache --timeout=864000
[branch]
	autosetuprebase = never
[push]
	default = simple
[core]
	editor = emacsclient --create-frame --no-wait --alternate-editor=''
	excludesfile = /Users/george/.gitignore_global
[diff]
	algorithm = patience
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
